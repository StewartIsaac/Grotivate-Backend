{
  "expo": {
    "name": "Grotivate",
    "slug": "Grotivate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "entryPoint": ".node_modules/expo/AppEntry.js",
    "splash": {
      "image": "./assets/mainSplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#47904E"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
       "output": "server"
    },
    "plugins": [
     [
      "expo-router",
      {
        "origin": "https://grotivate.onrender.com/"
      }
     ],
     [
      "expo-font",
      {
        "fonts": ["./assets/fonts/Inter.ttf","./assets/fonts/Sevillana.ttf"]
      }
        
     ]
    ],
    "scheme": "Grotivate"
  }
}
